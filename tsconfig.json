{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@components/*": ["./components/*"],
    //   "@styles/*": ["./styles/*"],
    //   "@public/*": ["./public/*"],
    //   "@utils/*": ["./utils/*"],
    //   "@hooks/*": ["./hooks/*"],
    //   "@assets/*": ["./assets/*"],
    //   "@context/*": ["./context/*"],
    //   "@pages/*": ["./pages/*"],
    //   "@lib/*": ["./lib/*"],
    //   "@services/*": ["./services/*"],
    //   "@config/*": ["./config/*"],
    //   "@constants/*": ["./constants/*"],
    //   "@types/*": ["./types/*"],
    //   "@tests/*": ["./tests/*"],
    //   "@mocks/*": ["./mocks/*"],
    //   "@interfaces/*": ["./interfaces/*"],
    //   "@models/*": ["./models/*"],
    //   "@controllers/*": ["./controllers/*"],
    //   "@middlewares/*": ["./middlewares/*"],
    //   "@routes/*": ["./routes/*"],
    //   "@validators/*": ["./validators/*"],
    //   "@helpers/*": ["./helpers/*"],
    },
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
